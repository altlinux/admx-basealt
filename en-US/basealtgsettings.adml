      <string id="org-mate-power-manager-action-sleep-type-battery_hibernate">hibernate</string>
      <string id="org-mate-power-manager-action-sleep-type-battery_interactive">interactive</string>
      <string id="org-mate-power-manager-action-sleep-type-battery_nothing">nothing</string>

      <string id="org-mate-power-manager-idle-dim-battery">Reducing the brightness of the screen when idle and powered by battery</string>
      <string id="org-mate-power-manager-idle-dim-battery_help">Set it to automatically reduce the brightness of the screen when idle and powered by battery</string>

      <string id="org-mate-power-manager-sleep-display-ac">The time of inactivity, in seconds, after which the display will be turned off when working from the network.</string>
      <string id="org-mate-power-manager-sleep-display-ac_help">Set the idle time in seconds after which the display will be turned off when working from the network.</string>

      <string id="org-mate-power-manager-notify-low-power">Low Battery Notification</string>
      <string id="org-mate-power-manager-notify-low-power_help">Set it if you want to receive a low battery notification</string>

      <string id="org-mate-power-manager-lock-blank-screen">Lock the screen when suspend</string>
      <string id="org-mate-power-manager-lock-blank-screen_help">Set it if you want to lock the screen when suspend. It is used only if the lock-usa-screensaver parameter is not set.</string>

      <string id="org-mate-power-manager-show-actions">Displaying parameters and statistics in the context menu</string>
      <string id="org-mate-power-manager-show-actions_help">Set it if you want to add parameters and statistics on power supply to the context menu.</string>

      <string id="org-mate-power-manager-info-stats-graph-smooth">Smoothing statistical data on the graph</string>
      <string id="org-mate-power-manager-info-stats-graph-smooth_help">Set it if you want to enable smoothing of statistical data on the chart</string>

      <string id="org-mate-power-manager-idle-dim-ac">Reducing the brightness of the screen when idle and powered by AC</string>
      <string id="org-mate-power-manager-idle-dim-ac_help">Set it to automatically reduce the brightness of the screen when idle and powered by AC</string>

      <string id="org-mate-power-manager-button-power">The action performed when the power button is pressed.</string>
      <string id="org-mate-power-manager-button-power_help">Set the action to be performed when the power button is pressed.</string>
