      <string id="org-mate-power-manager-button-power_suspend">suspend</string>
      <string id="org-mate-power-manager-button-power_shutdown">shutdown</string>
      <string id="org-mate-power-manager-button-power_hibernate">hibernate</string>
      <string id="org-mate-power-manager-button-power_interactive">interactive</string>
      <string id="org-mate-power-manager-button-power_nothing">nothing</string>

      <string id="org-mate-power-manager-idle-brightness">Screen brightness when the computer is idle</string>
      <string id="org-mate-power-manager-idle-brightness_help">Set the screen brightness when the computer is idle</string>

      <string id="org-mate-power-manager-time-action">The remaining battery life when a critical action is performed</string>
      <string id="org-mate-power-manager-time-action_help">The remaining battery life, in seconds, when a critical action is performed. Valid only if the use-time-for-policy parameter is set.</string>

      <string id="org-mate-power-manager-kbd-brightness-dim-by-on-battery">Reducing the brightness of the keyboard backlight when powered by a battery</string>
      <string id="org-mate-power-manager-kbd-brightness-dim-by-on-battery_help">The value to which the brightness of the keyboard should be lowered when powered by a battery, as a percentage. For example, if you set the value to "60", the backlight will be reduced by 40% when powered by a battery. Valid values are from 0 to 100.</string>

      <string id="org-mate-power-manager-sleep-computer-ac">The time in seconds before pausing the computer</string>
      <string id="org-mate-power-manager-sleep-computer-ac_help">Set the number of seconds that the computer should be idle when powered from the mains before pausing.</string>

      <string id="org-mate-power-manager-sleep-computer-battery">The time in seconds before pausing the computer when powered by battery</string>
      <string id="org-mate-power-manager-sleep-computer-battery_help">Set the number of seconds that the computer should be idle when powered by the battery before pausing.</string>

      <string id="org-mate-power-manager-time-critical">Critical time in seconds when powered by a battery</string>
      <string id="org-mate-power-manager-time-critical_help">The remaining battery life in seconds, perceived as critical. Valid only if the use-time-for-policy parameter is set.</string>

      <string id="org-mate-power-manager-notify-sleep-failed">Notification of an unsuccessful attempt to switch to sleep mode</string>
      <string id="org-mate-power-manager-notify-sleep-failed_help">Set it if you want to receive a notification about an unsuccessful attempt to switch to sleep mode.</string>

      <string id="org-mate-power-manager-button-hibernate">The action performed when the hibernation mode button is pressed.</string>
      <string id="org-mate-power-manager-button-hibernate_help">Set the action to be performed when the hibernation mode button is pressed.</string>
      <string id="org-mate-power-manager-button-hibernate_blank">blank</string>
      <string id="org-mate-power-manager-button-hibernate_suspend">suspend</string>
      <string id="org-mate-power-manager-button-hibernate_shutdown">shutdown</string>
      <string id="org-mate-power-manager-button-hibernate_hibernate">hibernate</string>
      <string id="org-mate-power-manager-button-hibernate_interactive">interactive</string>
      <string id="org-mate-power-manager-button-hibernate_nothing">nothing</string>

      <string id="org-mate-power-manager-use-time-for-policy">Using time-based notifications</string>
      <string id="org-mate-power-manager-use-time-for-policy_help">Whether time-based notifications should be used. If this parameter is not set, the battery charge percentage will be used instead of the time, which can fix the problem with the corrupted ACPI BIOS.</string>

      <string id="org-mate-power-manager-backlight-battery-reduce">Reducing the brightness of the screen when powered by a battery</string>
      <string id="org-mate-power-manager-backlight-battery-reduce_help">Set if you want to change the brightness of the screen when powered by battery.</string>

      <string id="org-mate-power-manager-notify-discharging">Notify when disconnecting from the AC power supply</string>
      <string id="org-mate-power-manager-notify-discharging_help">Set it if you want to receive a notification when disconnecting from the AC power supply.</string>

      <string id="org-mate-power-manager-dpms-method-ac">The DPMS method used when disconnecting the screen when powered from the mains.</string>
      <string id="org-mate-power-manager-dpms-method-ac_help">Select the DPMS method used when turning off the screen when powered from the mains.</string>
      <string id="org-mate-power-manager-dpms-method-ac_standby">standby</string>
      <string id="org-mate-power-manager-dpms-method-ac_suspend">suspend</string>
      <string id="org-mate-power-manager-dpms-method-ac_off">off</string>

      <string id="org-mate-power-manager-sleep-display-battery">The idle time of the computer before turning off the screen when powered by the battery</string>
      <string id="org-mate-power-manager-sleep-display-battery_help">Set the idle time of the computer in seconds before turning off the screen when powered by battery</string>

      <string id="org-mate-power-manager-percentage-low">The percentage of battery charge perceived as low</string>
      <string id="org-mate-power-manager-percentage-low_help">The percentage of battery charge perceived as low. Valid only if the use-time-for-policy parameter is not set.</string>

      <string id="org-mate-power-manager-action-low-ups">An action performed when the UPS is low.</string>
